{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
      "es2016",
      "es2017"
    ],
    "noUnusedLocals": false,
    "declaration": true,
    "declarationDir": "lib",
    "outDir": "lib",
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": ".",
    "incremental": true
  },
  "include": [
    "src"
  ]
}
